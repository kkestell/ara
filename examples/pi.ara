extern fn print_float(f: float) -> void

struct foo

fn pi(n: int) {
  pi := 4.0
  flag := false
  i := 3.0

  for c in 3..n {
    if flag {
      pi = pi + 4.0 / i
      flag = false
    } else {
      pi = pi - 4.0 / i
      flag = true
    }
    i = i + 2.0
  }

  return pi
}

fn main() {
  print_float(pi(10000))
}
